{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react",
    "lib": ["es6", "dom"],
    "removeComments": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  }
}

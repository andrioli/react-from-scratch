{
  "name": "react-from-scratch",
  "version": "1.0.0",
  "description": "React from scratch",
  "main": "index.js",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "clean": "rimraf dist",
    "precompile": "npm run clean",
    "compile": "webpack --config webpack.config.prod.ts",
    "start": "webpack-dev-server",
    "test": "jest",
    "coveralls": "coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/andrioli/react-from-scratch.git"
  },
  "author": "Roberto Araujo <roberto.andrioli@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/andrioli/react-from-scratch/issues"
  },
  "homepage": "https://github.com/andrioli/react-from-scratch#readme",
  "devDependencies": {
    "@types/deep-freeze": "^0.1.1",
    "@types/extract-text-webpack-plugin": "^2.1.0",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/jest": "^21.1.0",
    "@types/lodash": "^4.14.92",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-helmet": "^5.0.3",
    "@types/react-redux": "^5.0.8",
    "@types/react-router-dom": "^4.2.3",
    "@types/uuid": "^3.4.3",
    "@types/webpack": "^3.8.2",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "deep-freeze": "0.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.1.0",
    "node-sass": "^4.5.3",
    "redux-devtools-extension": "^2.13.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-jest": "^21.0.1",
    "ts-loader": "^2.3.4",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.3.3",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.0"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "todomvc-app-css": "^2.1.0",
    "uuid": "^3.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.scss$": "<rootDir>/jestSassTransform.js"
    },
    "testRegex": "\\.test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  }
}
